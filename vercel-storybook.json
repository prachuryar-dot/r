{
  "version": 2,
  "framework": null,
  "outputDirectory": "./",
  "installCommand": "echo 'No install needed for static files'",
  "buildCommand": "echo 'Build already completed'",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
